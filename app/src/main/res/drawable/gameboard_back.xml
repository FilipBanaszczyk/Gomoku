<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >

    <item>
        <shape android:shape="rectangle" >
            <corners
                android:radius="10dp"
                ></corners>
            <gradient
                android:angle="90"

                android:startColor="#02614E"
                android:gradientRadius="15"
                android:endColor="#7A0D79"
                android:type="linear" />

            <stroke
                android:width="0dp"
                android:color="#ff207d94" />
        </shape>
    </item>
    <item
        android:bottom="3dp"
        android:left="3dp"
        android:right="3dp"
        android:top="3dp">
        <shape android:shape="rectangle" >
            <corners
                android:radius="9dp"
                ></corners>
            <solid android:color="#373859" />
        </shape>
    </item>

</layer-list>
